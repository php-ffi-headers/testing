{
    "name": "ffi-headers/testing",
    "type": "library",
    "description": "PHP FFI Headers Testing Utils",
    "license": "MIT",
    "keywords": ["ffi", "testing", "utils"],
    "support": {
        "source": "https://github.com/php-ffi-headers/testing",
        "issues": "https://github.com/php-ffi-headers/testing/issues",
        "docs": "https://github.com/php-ffi-headers/testing/blob/master/README.md"
    },
    "authors": [
        {
            "name": "Nesmeyanov Kirill",
            "email": "nesk@xakep.ru",
            "homepage": "https://serafimarts.ru",
            "role": "maintainer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ffi/env": "^0.1|^1.0",
        "ffi/location": "^0.1|^1.0",
        "phpunit/phpunit": "^9.5.16"
    },
    "autoload": {
        "psr-4": {
            "FFI\\Headers\\Testing\\": "src"
        }
    },
    "require-dev": {
        "vimeo/psalm": "^4.22"
    },
    "autoload-dev": {
        "psr-4": {
            "FFI\\Headers\\Testing\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
